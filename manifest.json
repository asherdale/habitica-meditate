{
  "manifest_version": 2,

  "name": "Habitica-Meditation",
  "description": "Integration for Habitica and Insight Timer :)",
  "version": "1.0",
  "background": {
    "scripts": ["jquery-3.2.1.min.js", "jquery.csv.js", "moment.js", "eventPage.js"],
    "persistent": false
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "Click here!"
  },
  "permissions": [
    "activeTab",
    "storage",
    "alarms",
    "<all_urls>"
  ]
}
